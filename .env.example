# [required] Generate: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=


# [required] Postgres URL — https://vercel.com/docs/storage/vercel-postgres/quickstart
DATABASE_URL=

# [required for non-Vercel] Vercel AI Gateway — https://vercel.com/ai-gateway
AI_GATEWAY_API_KEY=

# [required] Vercel Blob token — https://vercel.com/docs/storage/vercel-blob
BLOB_READ_WRITE_TOKEN=

# [required for non-Vercel] - System Environment variables ---
# https://vercel.com/docs/environment-variables/system-environment-variables
VERCEL_URL=
VERCEL_PROJECT_PRODUCTION_URL=

# --- Auth (pick ≥1) ---
# Google — https://console.cloud.google.com/apis/credentials
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

# GitHub — https://github.com/settings/developers
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=


# --- Observability (Langfuse) ---
# Create project → Settings → API keys — https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
# Optional
LANGFUSE_BASE_URL=https://cloud.langfuse.com

# [optional] Generate: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
CRON_SECRET=

# --- Features ---
# [optional] E2B Code Interpreter - Create/push sandbox template; copy template ID — https://e2b.dev/docs
E2B_API_KEY=
SANDBOX_TEMPLATE_ID=
# [optional] Redis — https://vercel.com/docs/redis
REDIS_URL=
# [optional] OpenAI — https://platform.openai.com/api-keys
OPENAI_API_KEY=
# [optional] Tavily — https://app.tavily.com/
TAVILY_API_KEY=
# [optional] Exa — https://dashboard.exa.ai/
EXA_API_KEY=
# [optional] Firecrawl — https://www.firecrawl.dev/
FIRECRAWL_API_KEY=


